
{
    "name": "IntrvuFit - Resume optimizer",
    "version": "0.0.5",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_width": 800,
        "default_height": 600
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://www.linkedin.com/*",
        "https://*.linkedin.com/*",
        "http://localhost:8000/*"
    ]
}
